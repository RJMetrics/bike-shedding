{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "react": "^16.2.0",
    "react-burger-menu": "^2.3.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "react-table": "^6.8.0",
    "recharts": "^1.0.0-beta.10"
  },
  "scripts": {
    "start": "sass --watch src/styles/scss:src/styles/css | react-scripts start",
    "build": "yarn sass:build && react-scripts build",
    "sass:watch": "sass --watch src/styles/scss:src/styles/css",
    "sass:build": "sass --update src/styles/scss:src/styles/css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
